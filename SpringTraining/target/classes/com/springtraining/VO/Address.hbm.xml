<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.springtraining.VO.AddressVO" table = "addressbook">
<id name ="addressId" type="int" column = "AddressId">
<generator class = "increment"></generator></id>

<property name="line1">
			<column name="Line1"></column>
		</property>	
		
<property name="line2">
			<column name="Line2"></column>
		</property>	
<property name="zipcode">
			<column name="zipcode"></column>
		</property>	
<property name="firstname">
			<column name="firstname"></column>
		</property>	
		<property name="lastname">
			<column name="lastname"></column>
		</property>	
					
</class>		
</hibernate-mapping>